{
  "name": "eslintboilerplate",
  "version": "1.0.0",
  "description": "Boilerplate for JavaScript and TypeScript project from scratch, with ESLint/TSLint AirBNB Style.",
  "main": "app.js",
  "scripts": {
    "run": "parcel -p 4200 src/index.html",
    "build": "parcel build src/index.html --no-source-maps",
    "deploygithub": "parcel build src/index.html --no-source-maps --public-url /your-github-project-name/"
  },
  "keywords": [
    "boilerplate"
  ],
  "engines": {
    "node": ">=12.0.0"
  },
  "author": "gulajava_ministudio",
  "license": "MIT",
  "devDependencies": {
    "@babel/core": "^7.9.6",
    "@babel/plugin-transform-runtime": "^7.9.6",
    "@typescript-eslint/eslint-plugin": "^3.6.0",
    "@typescript-eslint/parser": "^3.6.0",
    "eslint": "^7.4.0",
    "eslint-config-airbnb": "^18.2.0",
    "eslint-config-node": "^4.1.0",
    "eslint-config-prettier": "^6.11.0",
    "eslint-plugin-import": "^2.22.0",
    "eslint-plugin-jsx-a11y": "^6.3.1",
    "eslint-plugin-node": "^11.1.0",
    "eslint-plugin-prettier": "^3.1.4",
    "eslint-plugin-react": "^7.20.3",
    "eslint-plugin-react-hooks": "^4.0.7",
    "parcel-bundler": "^1.12.4",
    "prettier": "^2.0.5",
    "typescript": "^3.9.6"
  },
  "dependencies": {
    "@babel/runtime": "^7.10.4"
  }
}
